  },
  "homepage": "https://github.com/airbnb/css#readme",
  "devDependencies": {
    "eslint": "^3.0.1",
    "eslint-config-airbnb-base": "^4.0.0",
    "eslint-plugin-import": "^1.10.2",
    "stylelint": "8.2.0",
