{
  "extends": [
    "@mate-academy/eslint-config-internal"
  ],
  "env": {
    "node": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ts"]
      },
      "alias": {
        "extensions": [".js", ".ts"],
        "map": [
          ["@", "."]
        ]
      }
    }
  },
  "rules": {
    "camelcase": "off"
  },
  "ignorePatterns": [
    "**/node_modules/*"
  ]
}
