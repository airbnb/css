{
  "name": "stylelint-config-airbnb",
  "version": "0.0.0",
  "description": "Airbnb's stylelint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "prelint": "editorconfig-tools check . test/",
    "lint": "eslint .",
    "pretest": "npm run --silent lint",
